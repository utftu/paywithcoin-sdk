{
	"namedInputs": {
		"default": ["{projectRoot}/**/*"],
		"production": ["!{projectRoot}/**/*.test.tsx"]
	},
	"targets": {
		"build": {
			"executor": "nx:run-script",
			"inputs": ["production", "^production"],
			"outputs": ["{workspaceRoot}/dist"],
			"dependsOn": ["^build"],
			"options": {
				"script": "build"
			}
		}
	}
}
